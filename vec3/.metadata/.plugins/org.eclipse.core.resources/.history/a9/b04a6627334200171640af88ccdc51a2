#include<iostream>
#include<cstdio>
#include<cmath>
using namespace std;
int main(){
	Vec3 v1,v2,result;
	v1 = Vec3(-1,1,2);
	v2 = Vec3(1,-1,4);
	result = v1.cross(v2);
	cout << result.getX() << " " << result.getY() << " " << result.getZ();
	return 0;

}



