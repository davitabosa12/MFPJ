#include"vec3.h"

Vec3::Vec3() {
	this->x = 0;
	this->y = 0;
	this->z = 0;
}

Vec3::Vec3(double x, double y, double z) {
	this->x = 0;
	this->y = 0;
	this->z = 0;
}

